{
  "name": "vue-touch-card-dl",
  "version": "0.1.5",
  "description": "vue directive for animating components",
  "main": "build/touchDirective.umd.js",
  "module": "build/touchDirective.esm.js",
  "unpkg": "build/touchDirective.min.js",
  "browser": {
    "./sfc": "touchDirective.js"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "prod": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file build/touchDirective.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file build/touchDirective.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file build/touchDirective.min.js"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoseJuan81/vue-touch-card-dl.git"
  },
  "keywords": [
    "vue",
    "directive",
    "touch",
    "effect",
    "card",
    "component"
  ],
  "author": "josejuan81",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoseJuan81/vue-touch-card-dl/issues"
  },
  "homepage": "https://github.com/JoseJuan81/vue-touch-card-dl#readme"
}
